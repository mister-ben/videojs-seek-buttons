{
  "name": "videojs-seek-buttons",
  "version": "1.2.0",
  "repository": "https://github.com/mister-ben/videojs-seek-buttons",
  "issues": "https://github.com/mister-ben/videojs-seek-buttons/issues",
  "description": "Plugin for video.js to add seek buttons to the control bar",
  "main": "dist/videojs-seek-buttons.cjs.js",
  "scripts": {
    "prebuild": "npm run clean",
    "build": "npm-run-all -p build:*",
    "build:css": "npm-run-all build:css:sass build:css:bannerize",
    "build:css:bannerize": "bannerize dist/videojs-seek-buttons.css --banner=scripts/banner.ejs",
    "build:css:sass": "node-sass src/plugin.scss dist/videojs-seek-buttons.css --output-style=compressed --linefeed=lf",
    "build:js": "npm-run-all build:js:rollup-modules build:js:rollup-umd build:js:bannerize build:js:uglify",
    "build:js:babel": "babel src -d es5",
    "build:js:bannerize": "bannerize dist/videojs-seek-buttons.js --banner=scripts/banner.ejs",
    "build:js:browserify": "browserify . -s videojs-seek-buttons -o dist/videojs-seek-buttons.js",
    "build:js:rollup-modules": "rollup -c scripts/modules.rollup.config.js",
    "build:js:rollup-umd": "rollup -c scripts/umd.rollup.config.js",
    "build:js:uglify": "uglifyjs dist/videojs-seek-buttons.js --comments --mangle --compress  -o dist/videojs-seek-buttons.min.js",
    "build:lang": "vjslang --dir dist/lang",
    "build:test": "rollup -c scripts/test.rollup.config.js",
    "build:test:browserify": "browserify  `find test -name '*.test.js'` -t babelify -o dist-test/videojs-seek-buttons.js",
    "clean": "rimraf dist test/dist",
    "postclean": "mkdirp dist test/dist",
    "docs": "npm-run-all -p docs:*",
    "docs:api": "documentation src/*.js -f html -o docs/api",
    "docs:toc": "doctoc README.md",
    "lint": "vjsstandard",
    "mkdirs": "mkdir -p dist dist-test es5",
    "prestart": "npm run build",
    "start": "npm-run-all -p start:server watch",
    "start:serve": "babel-node scripts/server.js",
    "start:server": "static -a 0.0.0.0 -p 9999 -H '{\"Cache-Control\": \"no-cache, must-revalidate\"}' .",
    "pretest": "npm-run-all lint build",
    "test": "karma start test/karma.conf.js",
    "preversion": "npm test",
    "version": "node scripts/version.js",
    "postversion": "git push origin master && git push origin --tags",
    "watch": "npm-run-all -p watch:*",
    "watch:css": "npm-run-all build:css:sass watch:css:sass",
    "watch:css:sass": "node-sass src/plugin.scss dist/videojs-seek-buttons.css --output-style=compressed --linefeed=lf --watch src/**/*.scss",
    "watch:js": "watchify src/plugin.js -t babelify -v -o dist/videojs-seek-buttons.js",
    "watch:js-modules": "rollup -c scripts/modules.rollup.config.js -w",
    "watch:js-umd": "rollup -c scripts/umd.rollup.config.js -w",
    "watch:test": "rollup -c scripts/test.rollup.config.js -w",
    "prepublish": "npm run build",
    "prepush": "npm run lint"
  },
  "keywords": [
    "videojs",
    "videojs-plugin"
  ],
  "author": "Ben Clifford",
  "license": "Apache-2.0",
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "qunit": "global:QUnit",
    "sinon": "global:sinon",
    "video.js": "global:videojs"
  },
  "vjsstandard": {
    "ignore": [
      "dist",
      "docs",
      "test/dist",
      "test/karma.conf.js"
    ]
  },
  "files": [
    "CONTRIBUTING.md",
    "dist/",
    "docs/",
    "index.html",
    "scripts/",
    "src/",
    "test/"
  ],
  "dependencies": {
    "global": "^4.3.2",
    "karma-safaritechpreview-launcher": "0.0.6",
    "video.js": "^5.19.2"
  },
  "devDependencies": {
    "babel": "^5.8.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-preset-env": "^1.6.1",
    "babelify": "^6.0.0",
    "bannerize": "^1.0.2",
    "browserify": "^11.0.0",
    "browserify-shim": "^3.0.0",
    "connect": "^3.4.0",
    "conventional-changelog-cli": "^1.3.1",
    "conventional-changelog-videojs": "^3.0.0",
    "cowsay": "^1.1.0",
    "doctoc": "^0.15.0",
    "documentation": "^3.0.0",
    "global": "^4.3.0",
    "husky": "^0.13.3",
    "karma": "^1.7.0",
    "karma-browserify": "^4.4.0",
    "karma-chrome-launcher": "^2.1.1",
    "karma-detect-browsers": "^2.2.5",
    "karma-firefox-launcher": "^1.0.1",
    "karma-ie-launcher": "^1.0.0",
    "karma-qunit": "^1.2.1",
    "karma-safari-launcher": "^1.0.0",
    "lodash-compat": "^3.10.0",
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1",
    "node-sass": "^4.5.3",
    "node-static": "^0.7.9",
    "npm-run-all": "^4.0.2",
    "portscanner": "^1.0.0",
    "qunitjs": "^2.3.2",
    "rimraf": "^2.6.1",
    "rollup": "^0.41.6",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-json": "^2.1.1",
    "rollup-plugin-multi-entry": "^2.0.1",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-watch": "^3.2.2",
    "semver": "^5.3.0",
    "serve-static": "^1.10.0",
    "sinon": "^2.2.0",
    "uglify-js": "^3.0.7",
    "videojs-languages": "^1.0.0",
    "videojs-standard": "^6.0.0",
    "watchify": "^3.6.0"
  },
  "module": "dist/videojs-seek-buttons.es.js",
  "generator-videojs-plugin": {
    "version": "5.0.2"
  }
}
